<?php

use \Drupal\Core\Entity\EntityInterface;

/**
 * Implements hook_ENTITY_TYPE_update().
 *
 * To replicate editor.module and catch save inceptions.
 */
function commerce_cart_test_commerce_order_update(EntityInterface $entity) {
  if (!$entity->isNew() && empty($entity->original)) {
    throw new \Exception('Found stale data! $entity->original not found on updated entity!');
  }
}
